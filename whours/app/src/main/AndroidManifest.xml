<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.instance.working.whours" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
<!--授予改程序录制声音的权限-->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme_ActionBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.ProjectListActivity"
            android:label="@string/acitivity_name_project_list" >
        </activity>
        <activity
            android:name=".view.ProjectActivity"
            android:label="@string/acitivity_name_project" >
        </activity>
        <activity
            android:name=".view.ItemActivity"
            android:label="@string/acitivity_name_item" >
        </activity>
        <activity
            android:name=".view.ItemListActivity"
            android:label="@string/acitivity_name_item_list" >
        </activity>

    </application>

</manifest>
